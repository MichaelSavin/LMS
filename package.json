{
  "name": "lms",
  "version": "0.0.0",
  "description": "LMS",
  "repository": {
    "type": "git",
    "url": "git://github.com/erohin/LMS.git"
  },
  "author": "Evgeny Erohin",
  "license": "",
  "scripts": {
    "npmcheckversion": "node ./internals/scripts/npmcheckversion.js",
    "preinstall": "npm run npmcheckversion",
    "postinstall": "npm run build:dll",
    "heroku-postbuild": "npm run build",
    "build": "cross-env NODE_ENV=production webpack --display-error-details --config internals/webpack/webpack.prod.babel.js --bail",
    "clean": "rimraf ./node_modules",
    "build:clean": "rimraf ./build",
    "build:dll": "node ./internals/scripts/dependencies.js",
    "start": "cross-env NODE_ENV=development node server",
    "start:monitor": "cross-env NODE_ENV=development webpack-dashboard -- node server",
    "start:production": "npm run build && npm run start:prod",
    "start:prod": "cross-env NODE_ENV=production node server",
    "postsetup": "npm run build:dll",
    "generate": "plop --plopfile internals/generators/index.js",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:eslint": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:css": "stylelint ./app/**/*.css",
    "lint:staged": "lint-staged"
  },
  "pre-commit": "lint:staged",
  "lint-staged": {
    "*.js": "lint:eslint",
    "*.css": "stylelint"
  },
  "dllPlugin": {
    "path": "node_modules/react-boilerplate-dlls",
    "exclude": [
      "chalk",
      "compression",
      "cross-env",
      "express",
      "ip",
      "minimist",
      "sanitize.css"
    ],
    "include": [
      "core-js",
      "lodash",
      "eventsource-polyfill"
    ]
  },
  "dependencies": {
    "antd": "2.3.0",
    "babel-polyfill": "6.16.0",
    "chalk": "1.1.3",
    "classnames": "2.2.5",
    "compression": "1.6.2",
    "draft-js": "0.9.0",
    "draftjs-utils": "0.3.2",
    "express": "4.14.0",
    "extensible-runtime": "4.0.1",
    "fontfaceobserver": "2.0.5",
    "immutable": "3.8.1",
    "invariant": "2.2.1",
    "ip": "1.1.3",
    "katex": "0.6.0",
    "lodash": "4.16.4",
    "rc-color-picker": "1.1.0",
    "react": "15.3.2",
    "react-dom": "15.3.2",
    "react-dropzone": "3.7.3",
    "react-helmet": "3.1.0",
    "react-immutable-proptypes": "2.1.0",
    "react-player": "0.12.0",
    "react-redux": "4.4.5",
    "react-router": "2.8.1",
    "react-router-redux": "4.0.6",
    "react-router-scroll": "0.3.2",
    "react-sortable-hoc": "0.1.1",
    "redux": "3.6.0",
    "redux-immutable": "3.0.8",
    "redux-saga": "0.12.0",
    "redux-storage": "4.1.1",
    "redux-storage-decorator-debounce": "1.1.1",
    "redux-storage-engine-localforage": "2.0.0",
    "redux-storage-engine-localstorage": "1.1.2",
    "redux-storage-merger-immutablejs": "1.0.3",
    "reselect": "2.5.4",
    "sanitize.css": "4.1.0",
    "warning": "3.0.0",
    "whatwg-fetch": "1.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.16.0",
    "babel-core": "6.17.0",
    "babel-eslint": "7.0.0",
    "babel-loader": "6.2.5",
    "babel-plugin-extensible-destructuring": "4.0.1",
    "babel-plugin-import": "1.0.1",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-transform-react-constant-elements": "6.9.1",
    "babel-plugin-transform-react-inline-elements": "6.8.0",
    "babel-plugin-transform-react-remove-prop-types": "0.2.10",
    "babel-preset-latest": "6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-0": "6.16.0",
    "cheerio": "0.22.0",
    "cross-env": "3.1.3",
    "css-loader": "0.25.0",
    "eslint": "3.8.1",
    "eslint-config-airbnb": "12.0.0",
    "eslint-config-cleanjs": "4.0.0",
    "eslint-import-resolver-webpack": "0.6.0",
    "eslint-plugin-better": "0.1.5",
    "eslint-plugin-fp": "2.2.0",
    "eslint-plugin-immutable": "1.0.0",
    "eslint-plugin-import": "2.0.1",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.4.1",
    "eventsource-polyfill": "0.9.6",
    "exports-loader": "0.6.3",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "html-loader": "0.4.4",
    "html-webpack-plugin": "2.22.0",
    "image-webpack-loader": "3.0.0",
    "imports-loader": "0.6.5",
    "json-loader": "0.5.4",
    "lint-staged": "3.2.0",
    "minimist": "1.2.0",
    "null-loader": "0.1.1",
    "offline-plugin": "3.4.2",
    "plop": "1.5.0",
    "postcss-cssnext": "2.8.0",
    "postcss-focus": "1.0.0",
    "postcss-loader": "1.0.0",
    "postcss-reporter": "1.4.1",
    "pre-commit": "1.1.3",
    "rimraf": "2.5.4",
    "shelljs": "0.7.4",
    "style-loader": "0.13.1",
    "stylelint": "7.5.0",
    "stylelint-config-css-modules": "0.1.0",
    "stylelint-config-standard": "13.0.2",
    "url-loader": "0.5.7",
    "webpack": "2.1.0-beta.15",
    "webpack-dashboard": "0.1.8",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.12.2"
  }
}
